{
  "name": "fs-open-forest-middlelayer-api",
  "version": "1.0.1",
  "description": "US Forest Service Open Forest Middlelayer API",
  "engines": {
    "node": "^10.18.0"
  },
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "test": "mocha test --recursive --exit",
    "test:mock": "npm --mock=Y run test",
    "coverage": "istanbul cover ./node_modules/mocha/bin/_mocha -- --recursive --exit",
    "create-admin": "node cmd/createUser.js -r admin",
    "lint": "npm-run-all -c -s lint:es lint:es:out lint:md lint:tsc",
    "lint:tsc": "tsc",
    "lint:es": "eslint . -f stylish",
    "lint:es:out": "eslint . -o ./lint/eslint.html -f html",
    "lint:es:fix": "eslint . --fix",
    "lint:md": "markdownlint ./docs ./README.md",
    "createdb": "sequelize db:create",
    "dba": "sequelize db:migrate && sequelize db:seed:all",
    "doc": "./node_modules/.bin/jsdoc -c .jsdoc.json -d docs/code -t ./node_modules/ink-docstrap/template -R README.md -r .",
    "postinstall": "npm run doc",
    "watch": "./node_modules/.bin/nodemon src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/18F/fs-middlelayer-api"
  },
  "keywords": [
    "gsa",
    "tts",
    "18f",
    "usda",
    "fs",
    "forest service",
    "epermit",
    "agile",
    "agile-bpa",
    "api",
    "node",
    "express"
  ],
  "author": "18F",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/18F/fs-middlelayer-api/issues"
  },
  "homepage": "https://github.com/18F/fs-middlelayer-api#readme",
  "devDependencies": {
    "@types/node": "^10.12.21",
    "aws-sdk-mock": "^2.0.0",
    "chai": "^3.5.0",
    "codecov": "^3.1.0",
    "eslint": "^3.19.0",
    "istanbul": "^0.4.5",
    "markdownlint-cli": "^0.8.2",
    "nodemon": "^1.18.9",
    "npm-run-all": "^4.1.5",
    "supertest": "^3.4.2",
    "typescript": "^3.4.3",
    "unionized": "^5.0.0"
  },
  "dependencies": {
    "async": "^2.6.1",
    "aws-sdk": "^2.398.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "deref": "^0.7.6",
    "dotenv": "^4.0.0",
    "express": "^4.16.4",
    "express-winston": "^2.6.0",
    "file-stream-rotator": "^0.1.0",
    "helmet": "^3.15.0",
    "include": "^1.1.0",
    "ink-docstrap": "^1.3.2",
    "jsdoc": "^3.6.3",
    "json-schema-deref-sync": "^0.6.0",
    "json-schema-faker": "^0.5.0-rc3",
    "jsonschema": "^1.1.1",
    "jsonwebtoken": "^8.4.0",
    "lodash": "^4.17.12",
    "matchstick": "^1.2.1",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "mocha": "^7.1.1",
    "mock-aws-s3": "^2.6.0",
    "morgan": "^1.9.1",
    "moxai": "^1.1.1",
    "multer": "^1.4.1",
    "newrelic": "^5.10.0",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "pg": "^6.2.2",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.5",
    "s3-zip": "^1.0.4",
    "sequelize": "^5.15.1",
    "sequelize-cli": "^5.5.0",
    "sinon": "^2.3.4",
    "url": "^0.11.0",
    "util": "^0.10.4",
    "uuid": "^3.3.2",
    "winston": "^2.4.4"
  }
}
